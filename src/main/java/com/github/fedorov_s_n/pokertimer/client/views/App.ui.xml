<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
    <ui:with field="bs" type="com.github.fedorov_s_n.pokertimer.client.resources.BaseStyle" />
    <gwt:HTMLPanel ui:field="root">
        <section class="{bs.app}">
            <div ui:field="headerPlaceholder"/>
            <section ui:field="main">
                <table>
                    <tr>
                        <td width="*">
                            <div ui:field="leftPlaceholder" />                        
                        </td>
                        <td width="520" class="rspanel">
                            <div ui:field="rightPlaceholder" />
                        </td>
                    </tr>
                </table>
            </section>
        </section>
        <section class="{bs.info}">
            <div ui:field="infoPlaceholder" />
        </section>
    </gwt:HTMLPanel>
</ui:UiBinder>